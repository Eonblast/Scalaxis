<erl>
out(A) ->
    IsPost = webhelpers:isPost(A),
    if
      	IsPost ->
		{ok, Node} = postvar(A,"node"),
		case Node of
		  "processTreeRoot" ->
		    {html, json:encode(pid_groups:groups_as_json())};
		  _ ->
		    {html, json:encode(pid_groups:members_by_name_as_json(Node))}
		end;
	true ->
      		{ehtml, 
       			{pre, [], 
        		""}}
    end.
</erl>
