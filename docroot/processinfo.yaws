<erl>
out(A) ->
    IsPost = webhelpers:isPost(A),
    {value, {_, Group}} = lists:keysearch("group", 1, yaws_api:parse_query(A)),
    {value, {_, Id}} = lists:keysearch("id", 1, yaws_api:parse_query(A)),
    if 
	Group == undefined -> 
	  {ehtml, 
       	    {pre, [], 
            ""}}; 
	Id == undefined ->
      	  {ehtml, 
       		{pre, [], 
        	""}};
	true ->
	      {html, json:encode(process_dictionary:get_info(Group, Id))}
     end.
</erl>
